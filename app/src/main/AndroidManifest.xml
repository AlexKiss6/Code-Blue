<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alex.codeblue">

    <!-- permissions go here -->
    <!-- sample permission: <uses-permission android:name="android.permission.GET_ACCOUNTS" /> -->
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AssistInputActivities.JumpstartActivity"
            android:label="@string/title_activity_jumpstart"
            android:theme="@style/MyMaterialTheme"></activity>
        <activity
            android:name=".AssistInputActivities.AnimalControlActivity"
            android:label="@string/title_activity_animal_control"
            android:theme="@style/MyMaterialTheme"></activity>
        <activity
            android:name=".AssistInputActivities.EscortActivity"
            android:label="@string/title_activity_escort"
            android:theme="@style/MyMaterialTheme"></activity>
        <activity
            android:name=".AssistInputActivities.NoiseComplaintActivity"
            android:label="@string/title_activity_noise_complaint"
            android:theme="@style/MyMaterialTheme"></activity>
        <activity
            android:name=".AssistInputActivities.RoomLockoutActivity"
            android:label="@string/title_activity_room_lockout"
            android:theme="@style/MyMaterialTheme"></activity>
        <activity
            android:name=".AssistInputActivities.VehicleLockoutActivity"
            android:label="@string/title_activity_vehicle_lockout"
            android:theme="@style/MyMaterialTheme"></activity>
    </application>

</manifest>
